{
    "builds": [{
      "src": "MedCom/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb"
      }
    }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "MedCom/wsgi.py"
      }
    
    
    ],
  
    "buildCommand": "python manage.py collectstatic --noinput"
  }

  